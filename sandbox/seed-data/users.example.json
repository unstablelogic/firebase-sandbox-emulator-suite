  "defaultPreferences": {
    "theme": "light",
    "notifications": true,
    "language": "en",
    "timezone": "UTC"
  },
  "defaultSettings": {
    "emailVerified": false,
    "profileComplete": false,
    "lastLogin": null,
    "twoFactorEnabled": false
